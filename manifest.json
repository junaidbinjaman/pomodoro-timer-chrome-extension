{
    "manifest_version": 3,
    "name": "Pomodoro Timer",
    "version": "1.0.0",
    "description": "A pomodoro timer extension for google chrome",
    "icons": {
      "16": "images/icon.png",
      "48": "images/icon.png",
      "128": "images/icon.png"
    },
    "action": {
        "default_icon": {
          "16": "images/icon.png",
          "32": "images/icon.png",
          "48": "images/icon.png",
          "128": "images/icon.png"
        },
        "default_popup": "popup/popup.html"
      }
  }